    "@types/node-cache": "^4.1.3",
    "@types/nodemailer": "^6.4.17",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.5",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "critters": "^0.0.25",
    "embla-carousel-react": "^8.5.1",
    "ioredis": "^5.3.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.468.0",
    "next": "^15.0.3",
