{
  "name": "nextjs-woo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev 2>&1 | tee -a logs/dev-$(date +%Y%m%d-%H%M%S).log",
    "build": "next build",
    "start": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=12288 --max-http-header-size=24576' pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "monitor": "pm2 monit",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "type-check": "tsc --noEmit",
    "check-all": "yarn type-check && yarn lint",
    "deploy": "yarn build && yarn start"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
